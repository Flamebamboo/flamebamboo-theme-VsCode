{
  "name": "FlameBamboo",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#d3b8b8"
    },
    "variable.constant": {
      "foreground": "#06d0fd"
    },
    "variable.defaultLibrary": {
      "foreground": "#06d0fd"
    }
  },
  "tokenColors": [
    {
      "name": "Variables and Props",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.parameter",
        "support.variable.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#E8ECD7"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function.js",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#FFF7D1"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof"
      ],
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#9799ff"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#FC8F54"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.inline.any.html",
        "meta.tag.block.any.html",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#06d0fd"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#06d0fd"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.separator.delimiter",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.css",
        "keyword.operator.scss",
        "keyword.operator.less"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": "support.type.vendored.property-name.css",
      "settings": {
        "foreground": "#CDC1FF"
      }
    },
    {
      "name": "JS/TS Modules",
      "scope": ["support.module.node", "support.type.object.module"],
      "settings": {
        "foreground": "#00cfff"
      }
    },
    {
      "name": "Entity Name Type Module",
      "scope": "entity.name.type.module",
      "settings": {
        "foreground": "#00cfff"
      }
    },
    {
      "name": "JS Variable Readwrite",
      "scope": [
        "variable.other.readwrite",
        "meta.object-literal.key",
        "support.variable.property",
        "support.variable.object.process",
        "support.variable.object.node"
      ],
      "settings": {
        "foreground": "#e46060"
      }
    },
    {
      "name": "JS/TS JSON",
      "scope": "support.constant.json",
      "settings": {
        "foreground": "#06d0fd"
      }
    },
    {
      "name": "JS/TS Console",
      "scope": "support.type.object.console",
      "settings": {
        "foreground": "#e46060"
      }
    },
    {
      "name": "JS/TS Support Variable Property Process",
      "scope": "support.variable.property.process",
      "settings": {
        "foreground": "#06d0fd"
      }
    },
    {
      "name": "JS Console Function",
      "scope": ["entity.name.function", "support.function.console"],
      "settings": {
        "foreground": "#C2FFC7"
      }
    },
    {
      "name": "JS DOM",
      "scope": "support.type.object.dom",
      "settings": {
        "foreground": "#CDC1FF"
      }
    },
    {
      "name": "JS DOM Variable",
      "scope": ["support.variable.dom", "support.variable.property.dom"],
      "settings": {
        "foreground": "#e46060"
      }
    },
    {
      "name": "HTML Meta Tag",
      "scope": "meta.tag",
      "settings": {
        "foreground": "#ffffff"
      }
    },

    {
      "name": "HTML Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#13cfc6"
      }
    },
    {
      "name": "HTML Bold",
      "scope": ["markup.bold", "todo.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#06d0fd"
      }
    },
    {
      "name": "HTML Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic",
        "todo.emphasis"
      ],
      "settings": {
        "foreground": "#45d97f"
      }
    },
    {
      "name": "Markdown Elements",
      "scope": [
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown"
      ],
      "settings": {
        "foreground": "#e46060"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#13cfc6"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d3b8b8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#d3b8b8"
      }
    },
    {
      "name": "Embedded Code",
      "scope": [
        "punctuation.section.embedded",
        "variable.interpolation",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#e46060"
      }
    },
    {
      "name": "Illegal Characters",
      "scope": ["invalid.illegal", "invalid.illegal.bad-ampersand.html"],
      "settings": {
        "foreground": "#ffffff"
      }
    }
  ],
